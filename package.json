{
  "name": "es6-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for a transpiled ES2016 library",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "test": "standard && mocha -r babel-register -R spec --bail",
    "coverage": "nyc --reporter=lcov npm test",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SomeoneWeird/es6-boilerplate.git"
  },
  "keywords": [
    "es6",
    "es2016",
    "boilerplate",
    "template"
  ],
  "author": "Adam Brady <adam@boxxen.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SomeoneWeird/es6-boilerplate/issues"
  },
  "homepage": "https://github.com/SomeoneWeird/es6-boilerplate#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4",
    "standard": "^7.1.1"
  }
}
